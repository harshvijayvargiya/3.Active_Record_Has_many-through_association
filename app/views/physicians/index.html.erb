<%= link_to "New Physician" , new_physician_path %>
<%= link_to "Back" , controller: "welcome" %>

<h1>Listing All Physician</h1>
 
<table>
  <tr>
    <th>Name Of Physician</th>
    <th></th>
  </tr>
 
  <% @physicians.each do |physician| %>
    <tr>
      <td><%= physician.name %></td>
      <td><%= link_to 'Show', physician_path(physician) %></td>
      <td><%= link_to 'Edit', edit_physician_path(physician) %></td>
      <td><%= link_to 'Destroy', physician_path(physician), method: :delete, data: {confirm: "Are You Sure..?"} %></td>
    </tr>
  <% end %>
</table>